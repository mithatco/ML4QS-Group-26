    "import os\n",
    "\n",
    "# show all csv files one by one:\n",
    "path = \"\" # current directory\n",
    "for file in os.listdir(path):\n",
    "    if file.endswith(\".csv\"):\n",
    "        print(file)\n",
    "        df = pd.read_csv(file)\n",
    "        print(df.head())\n",
    "        print(\"\\n\")"
